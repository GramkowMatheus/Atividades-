class Data:
    def __init__(self, dia, mes, ano, cidade):
        self.dia = dia 
        self.mes = mes   
        self.ano = ano 
        self.cidade = cidade
    def nac(self):
        print(self.dia,"/",self.mes,"/",self.ano,"-",self.cidade )
    

diad = int(input("Digite o dia de hoje: "))
mesd = int(input("Digite o mÃªs que estamos: "))
anod = int(input("Digite o ano que estamos "))
cidaded = input("Digite o nome da sua cidade: ")
datad =Data(diad,mesd,anod,cidaded)
datad.nac()
print(datad.nac)



